@{
	ViewData["Title"] = "Index";
}
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<div>
	<button onclick="post()" type="button">Post Test</button>
	<button onclick="put()" type="button">Put Test</button>
	<button onclick="get()" type="button">Get Test</button>
	<button onclick="guid()" type="button">GUID Test</button>
	<button onclick="ZestOfBitchi()" type="button">ZestOfBitchi</button>
